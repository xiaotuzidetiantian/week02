<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wuxufang.dao.UserMapper">

	<insert id="insert">
		insert into user(id,name,sex,phone,email,birthday)
		values(#{id},#{name},#{sex},#{phone},#{email},#{birthday})
	</insert>
</mapper>